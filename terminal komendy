('wiking2','wiking'),
('wiking3','wiking'),
('wiking4','wiking'),
('wiking5','wiking');


select * from postac1 wiking2;


create table statek (
nazwa_statku varchar(40) primary key,
rodzaj_statku enum('transportowy','rybacki'),
data_wodowania date,
max_ladownosc int unsigned);


insert into statek (nazwa_statku)
values
('statek1'),
('statek2');

select * from statek statek2;


alter table postac1 add funkcja varchar(40);

update postac1 
set funkcja = 'kapitan'
where id_postaci = 1;


alter table postac1
add column statek;



